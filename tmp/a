## Returns true if objects can be instantiated from the specified class, otherwise returns false.
static func can_instantiate(name: StringName) -> bool:
	match query_class(name):
		ClassQueryResult.CLASS_DB:
			return ClassDB.
		ClassQueryResult.CUSTOM_SCRIPT
			pass
		push_warning("class named %s not found" % name)
	return []


## Calls a static method on a class.
static func class_call_static(name: StringName, method: StringName, ...args: Array) -> Variant:
	match query_class(name):
		ClassQueryResult.CLASS_DB:
			return ClassDB.
		ClassQueryResult.CUSTOM_SCRIPT
			pass
		push_warning("class named %s not found" % name)
	return []


